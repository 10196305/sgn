<%args>

</%args>

<& /util/import_javascript.mas, classes => ["jqueryfileupload.vendor.ui-widget", "jqueryfileupload.iframe-transport",   "jqueryfileupload.fileupload",   "solGS.userReferenceGenotypes"] &>

<!-- The fileinput-button span is used to style the file input field as button --> 
 <div class="fileupload-buttonbar">
    <span class="btn btn-success fileinput-button">
        <i class="glyphicon-plus"></i>
        <span>Upload reference genotypes</span>
        <!-- The file input field used as target for the file upload widget -->        
        <input id="fileupload"  type="file" name="files[]" multiple>
        <input type="hidden" id="check_value" value=""> 
    </span>
   
   
    <div class="list_upload" id="list_label">
      <label for="reference_genotypes_list">Or select a list </label>
    </div> 
    <div class="list_upload" id="reference_genotypes_list">      
    </div>
    <div class="list_upload" id="reference_genotypes_list_upload">
      <input type="button" value="Upload" />
    </div>
    <br>
    <br>
   
    <!-- The container for the uploaded files -->
    <div class="sub_infosectioncontent" id="uploaded_reference_populations" style="display:none"></div>
    <br>

</div>



