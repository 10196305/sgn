<%doc>

=head1 NAME
 
population/trait_phenotype_data.mas - displays scatter plot of trait phenotype data

=AUTHOR

Isaak Y Tecle (iyt2@cornell.edu)

=cut

</%doc>

<%args>

$pop_id => ''
$trait_id
$descriptive_stat
$combo_pops_id => ''
</%args>

<%perl>


</%perl>

 <script>
   jQuery(function() {
   jQuery("#tabs_phenotype").tabs({
   event: "mouseover"
   });
   });

</script>


<div id="tabs_phenotype" style="width:100%;height:100%;border-color:green;">
  <ul>
    <li><a href="#tabs_scatter">Scatter plot</a></li>
    <li><a href="#tabs_histogram">Histogram</a></li> 
  </ul>
 
  <div id="tabs_scatter">
    <& trait_phenotype_scatter.mas,
       pop_id => $pop_id,
      trait_id => $trait_id,
      descriptive_stat => $descriptive_stat
      &>
  </div>

  <div id="tabs_histogram">
    <& trait_phenotype_histogram.mas,
       pop_id => $pop_id,
      trait_id => $trait_id
      &>
  </div>
  
  <br/>
  
  <& /page/columnar_table.mas,
     headings    => [ '',  ''],
    data         => $descriptive_stat,
    __alt_freq   => 2,
    __alt_width  => 1,
    __alt_offset => 3,
    __align      => 'l',
    &>

    <br/>

</div>







