<& /page/page_title.mas, title=>"Carry out quality control genotyping" &>

<& /util/import_javascript.mas, classes => [ 'jquery.iframe-post-form', 'CXGN.BreedersToolbox.GenotypingTrial' ] &>

<div class="container-fluid">

    <&| /page/info_section.mas, title=>"QC assay upload",  collapsible => 1, collapsed=>0 &>
