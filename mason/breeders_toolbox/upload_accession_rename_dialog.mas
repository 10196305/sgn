<%args>
</%args>

<div class="modal fade" id="upload_accession_name_change_validate_display" name="upload_accession_name_change_validate_display" tabindex="-1" role="dialog" aria-labelledby="uploadGraftValidateDialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="uploadAccessionNameChangeValidateDialog">Results From Validation</h4>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
          <!-- div class="well">To Overwrite previous scion and/or rootstocks, in the case that an accession already has scions/rootstocks <input type="checkbox" checked id="graft_upload_overwrite_graft" /></div -->
 	  <table class="table table-hover">
    	    <tbody></tbody>
  	  </table>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" name="upload_accession_name_change_store" id="upload_accession_name_change_store" data-dismiss="modal">Store</button>
	
      </div>
    </div>
  </div>
</div>


<div class="modal fade" id="accession_name_change_upload_success_dialog_message" name="graft_upload_success_dialog_message" tabindex="-1" role="dialog" aria-labelledby="uploadAccessionNameChangeSuccessDialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="uploadAccessionNameChangeSuccessDialog">Upload Successful</h4>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
 	  <h2>The accession change list upload was successful.</h2>

	  Successfully created:<br />
	  <textarea id="list_of_uploaded_accession_change_changes">
	  </textarea>
	  <br />
	  
	  Already in the system:<br />
	  <textarea id="list_of_already_uploaded_accession_name_changes">
	  </textarea>

	  <div id="list_of_accession_name_changes_with_problem"></div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" id="accession_name_changes_upload_success_dismiss" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>


<div class="modal fade" id="upload_graft_dialog" name="upload_graft_dialog" tabindex="-1" role="dialog" aria-labelledby="uploadGraftDialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="uploadGraftDialog">Upload Accession name change Information</h4>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
    	  <&| /page/explanation.mas, title=>'Template Information' &>
    	  <p>
      	  <b>File format information</b>
      	  <br>
      	  <a id="graft_upload_spreadsheet_format_info" data-toggle='modal' data-target='#graft_upload_spreadsheet_format_info_dialog'>Spreadsheet Format</a>
    	  </p>
  	  </&>

  	  <form method="post" enctype="multipart/form-data" encoding="multipart/form-data" id="upload_accession_name_change_form" name="upload_accession_name_change_form">
    	    <label for="accession_name_change_uploaded_file" style="display: inline-block; width: 100px;">Upload Accession Name Change File:</label>
    	    <input type="file" name="accession_name_change_uploaded_file" id="accession_name_change_uploaded_file" encoding="multipart/form-data" />
    	  </form>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
	<button type="button" class="btn btn-primary" name="upload_accession_name_change_dialog_submit" id="upload_accession_name_change_dialog_submit">Validate</button>
      </div>
    </div>
  </div>
</div>

<div class="modal fade" id="accession_name_change_upload_spreadsheet_format_info_dialog" name="accession_name_change_upload_spreadsheet_info_dialog" tabindex="-1" role="dialog" aria-labelledby="accessionNameChangeUploadInfoDialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="graftUploadInfoDialog">Template Information</h4>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
 	  <b>Graft may be uploaded in tab-delimited text file format</b>
	  <br>
	  <p>(.xls or .xlsx formats are <b>NOT</b> supported)</p>
	  <hr>
	  <b>Header:</b>
	  <br>
	  <p>The first row (header) should contain the following:</p>

	  <table class="table table-hover table-bordered">
  	  <body>
	    <tr>
    	      <td>old_name</td>
              <td>new_name</td>
  	    </tr>
	  </body>
	  </table>

	  <h4>Required fields:</h4>
	  <ul>
        <li>old accession name (exact name must exist in the database)</li>
        <li>new accession name (must NOT exist in the database)</li>
      </ul>

      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>

