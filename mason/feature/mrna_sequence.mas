<%args>
    $mrna_feature
    $title => 'mRNA Sequence'
    $collapsible => 1
</%args>
<%perl>
    use SGN::View::Feature qw/mrna_sequence/;
    my $mrna_sequence = mrna_sequence($mrna_feature);
</%perl>

<& /feature/sequence.mas,
    title     => $title,
    sequence  => $mrna_sequence,
    blast_url => undef,
    id        => $mrna_feature->name,
&>
