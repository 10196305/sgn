<%args>
  $feature
</%args>
% if( $feature->organism ) {
  <& /page/page_title.mas, title => '<i>'.$feature->organism->species.'</i> '.type_name($feature).' '. $feature->id &>
% } else {
  <& /page/page_title.mas, title => type_name($feature,'caps').' '. $feature->id &>
% }
<%once>
    use SGN::View::Feature qw/ type_name /;
</%once>
