<div class="boxbgcolor1" style="border:0px; ">
<div class="boxheading">GitHub activity</div>
<div id="github_activity" class="boxcontent">loading ...</div>
<script language="javascript" type="text/javascript">
jQuery(function($) {
    $.ajax({
      "url": '/github/org_news/solgenomics',
      "contentType": 'text/html',
      "success": function(html) {
        $('#github_activity')[0].innerHTML = html;
      },
      "error": function() {
        $('#github_activity')[0].innerHTML = '<span class="ghosted">temporarily unavailable</span>'
      }
    })
});
</script>
<& /util/import_javascript.mas, classes => 'jquery' &>
